{
  "name": "corelink",
  "version": "1.0.0",
  "scripts": {
    "start": "webpack serve --mode development",
    "start:live": "webpack serve --mode development --live-reload --hot",
    "build": "webpack --mode production --config webpack.config.prod.js",
    "serve": "serve -l 3000 -s dist",
    "bs": "npm run build && npm run serve",
    "test": "vitest --run --config ./vitest.config.js ./tests/unit/ && npx playwright test",
    "test:unit": "vitest --config ./vitest.config.js ./tests/unit/",
    "test:e2e": "npx playwright test",
    "test:snapshot": "npx playwright test --update-snapshots"
